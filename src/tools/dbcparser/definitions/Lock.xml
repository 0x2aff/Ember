<?xml version="1.0"?>
<Lock>
	<field>
		<type>uint32</type>
		<name>id</name>
		<key>
			<type>primary</type>
		</key>
	</field>

	<field>
		<type>enum32[8]</type>
		<name>type</name>
		<options>
			<option name="none" value="0x00" />
			<option name="item_required" value="0x01" />
			<option name="locktype_reference" value="0x02" />
		</options>		
	</field>

	<field>
		<type>uint32[8]</type>
		<name>property</name>
		<key>
			<type>foreign</type>
			<parent>LockType</parent>
		</key>
	</field>

	<field>
		<type>int32[8]</type>
		<name>required_skill</name>
	</field>

	<field>
		<type>int32[8]</type>
		<name>action</name>
	</field>
</Lock>