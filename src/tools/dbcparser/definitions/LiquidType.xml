<?xml version="1.0"?>
<LiquidType>
	<field>
		<type>uint32</type>
		<name>id</name>
		<key>
			<type>primary</type>
		</key>
	</field>

	<field>
		<type>string_ref</type>
		<name>name</name>
	</field>

	<field>
		<type>enum32</type>
		<name>type</name>
		<options>
			<option name="fire" value="0x00" />
			<option name="slime" value="0x02" />
			<option name="water" value="0x04" />
		</options>
	</field>

	<field>
		<type>uint32</type>
		<name>spell</name>
		<key>
			<type>foreign</type>
			<parent>Spell</parent>
		</key>
	</field>
</LiquidType>