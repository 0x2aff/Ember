<?xml version="1.0"?>
<EmotesTextSound>
	<field>
		<type>uint32</type>
		<name>id</name>
		<key>
			<type>primary</type>
		</key>
	</field>

	<field>
		<type>uint32</type>
		<name>emotes_text</name>
		<key>
			<type>foreign</type>
			<parent>EmotesText</parent>
		</key>
	</field>

	<field>
		<type>uint32</type>
		<name>race</name>
		<key>
			<type>foreign</type>
			<parent>ChrRaces</parent>
		</key>
	</field>

	<field>
		<type>enum32</type>
		<name>sex</name>
		<options>
			<option name="male" value="0x00" />
			<option name="female" value="0x01" />
		</options>
	</field>

	<field>
		<type>uint32</type>
		<name>sound</name>
		<key>
			<type>foreign</type>
			<parent>SoundEntries</parent>
		</key>
	</field>
</EmotesTextSound>